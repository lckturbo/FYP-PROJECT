    choiceB:
      text: 
      nextDialogue: {fileID: 0}
  - speakerName: High Guard (offscreen)
    text: There you are, intruders.
    portrait: {fileID: 0}
    portraitSide: 1
    hasChoices: 0
    choiceA:
      text: 
      nextDialogue: {fileID: 0}
    choiceB:
      text: 
      nextDialogue: {fileID: 0}
  - speakerName: High Guard
    text: "So it\u2019s you lot who\u2019ve been causing trouble around this wretched
